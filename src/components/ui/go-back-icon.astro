---
type Props = {
  href?: string;
};

const { href = "/" } = Astro.props;
---

<a
  href={href}
  class="bg-muted group w-fit rounded-md border border-white/10 p-1"
>
  <img
    alt="go back arrow icon"
    src="/svg/go-back-arrow.svg"
    class="size-4 opacity-50 transition group-hover:opacity-100"
  /></a
>
